{
  "buildCommand": "cd apps/docs && npm install && npm run build",
  "outputDirectory": "apps/docs/build",
  "devCommand": "cd apps/docs && npm start",
  "installCommand": "npm install -g pnpm && pnpm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

